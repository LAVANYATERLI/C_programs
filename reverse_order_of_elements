#include<stdio.h>
int main()
{
    int n,i,a[100];
    printf("enter n no.of values:\n");
    scanf("%d",&n);
    printf("enter the values:\n",n);
    for(i=0;i<n;i++)
    {
    printf("element-%d\t",i);
    scanf("%d",&a[i]);
    }
    printf("the values stored n array:\n");
    for(i=0;i<n;i++)
    {
    printf("%5d",a[i]);
    }
    printf("the values stored in reverse order\n");
    for(i=n-1;i>=0;i--)
    {
        printf("%5d",a[i]);
    }
    printf("\n\n");
    return 0;
}
    
