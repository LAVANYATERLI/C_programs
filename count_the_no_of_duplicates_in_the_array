#include<stdio.h>
int main() 
{
    int a[100],n,i,j,c=0;
    printf("Enter the number of elements in array:");
    scanf("%d",&n);
    printf("Enter %d integers:\n",n);
    for(i=0;i<n;i++) 
    {
    scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
    for(j=i+1;j<n;j++)
    {
    if(a[i]==a[j])
    {
    c++;
    break;
    }
    }
    }
    printf("The total no.of duplicate elements found:%d",c);
    return 0;
}

    
